products_change);\r\n            setCart(old_cart);\r\n        }\r\n\r\n    }\r\n\r\n    function checkOut(e) {\r\n        e.preventDefault();\r\n\r\n        var totalQuantity = 0;\r\n        cart.map((value => {\r\n            totalQuantity += value.quantity;\r\n        }))\r\n\r\n        if (totalQuantity < 20) {\r\n            setMessage('Include 20KG atleast');\r\n            document.getElementById('order-message').style.color = 'red';\r\n            return\r\n        }\r\n\r\n        axios({\r\n            method: 'post',\r\n            url: `${url}/place-order`,\r\n            data: {\r\n                cart: cart,\r\n                address: address.current.value,\r\n                phoneNo: phoneNo.current.value,\r\n                quantity: totalQuantity,\r\n                remarks: remarks.current.value,\r\n            },\r\n\r\n        }).then((response) => {\r\n            console.log(\"response is = > \", response.data);\r\n            setMessage(\"Your request has been placed\");\r\n            document.getElementById('order-message').style.color = 'black';\r\n\r\n            cart.map((value) => {\r\n                value.quantity = 0\r\n                setCart([]);\r\n            });\r\n\r\n        }, (error) => {\r\n            console.log(\"an error occured\");\r\n        })\r\n        products.map((value) => value.added = false);\r\n    }\r\n\r\n\r\n\r\n    return (\r\n        <div>\r\n            <div className=\"wrapper\">\r\n                <nav className=\"navbar navbar-expand-lg navbar-light bg-light\">\r\n                    <div className='container'>\r\n                        <a className=\"navbar-brand\" href=\"#\">{globalState.user.userName}</a>\r\n                        <button className=\"navbar-toggler\" type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarText\" aria-controls=\"navbarText\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\r\n                            <span className=\"navbar-toggler-icon\" />\r\